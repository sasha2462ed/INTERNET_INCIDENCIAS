        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txvv9"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/txvv6"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:gravity="center|right"
        android:text="@string/txvv6"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txvv4"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <ImageView
        android:id="@+id/maps"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:paddingLeft="1dp"
        android:paddingRight="2dp"
        android:rotation="90"
        android:src="@drawable/ic_baseline_navigation_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvmc5"
        app:layout_constraintTop_toBottomOf="@+id/tvmc4"
        tools:ignore="MissingConstraints" />

    <ImageView
