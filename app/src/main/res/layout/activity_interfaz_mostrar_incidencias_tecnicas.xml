        android:layout_marginStart="8dp"
        android:gravity="center|left"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/maps"
        app:layout_constraintStart_toEndOf="@+id/txvv8"
        app:layout_constraintTop_toBottomOf="@+id/tvmc4"
        tools:ignore="HardcodedText,RtlHardcoded" />

    <TextView
        android:id="@+id/tvm8"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:gravity="center|left"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/btnestado"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/tvmc7"
